operation EClass link(){
	if(self.isAnnotatedAs("gmf.link")){
		var e:EReference= EReference.all.selectOne(t|t.eContainingClass.isUndefined());
		if(e.isDefined()){
			e.eType=self;
			EClass.all.selectOne(c|c.isAnnotatedAs('gmf.diagram')).eStructuralFeatures.add(e);
			//e.eContainingClass= EClass.all.selectOne(c|c.isAnnotatedAs('gmf.diagram'));
		}
	}
	
}
operation EClass link(){
	if(self.isAnnotatedAs("gmf.diagram")){
		var e:EReference= EReference.all.selectOne(t|t.eContainingClass.isUndefined());
		if(e.isDefined()){
			e.eType=EClass.all.selectOne(c|c.isAnnotatedAs('gmf.diagram'));
			self.eStructuralFeatures.add(e);
		}
	}
}
