	if(self.source="gmf.compartment"){
		var e:EReference= EReference.all.selectOne(t|t.eContainingClass.isDefined() and t.isNotAnnotated());
		if(e.isDefined()){
			e.eAnnotations.add(self);
		}
	}
	
}
